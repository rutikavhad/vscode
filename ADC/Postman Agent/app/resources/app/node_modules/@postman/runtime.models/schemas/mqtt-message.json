{
	"$id": "mqtt-message",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
		"type": {
			"type": "string",
			"const": "mqtt-message"
		},
		"id": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"workspace": {
			"type": "string"
		},
		"createdAt": {
			"type": "string"
		},
		"updatedAt": {
			"type": "string"
		},
		"payload": {
			"type": "object",
			"properties": {
				"payload": {
					"type": "string"
				},
				"topic": {
					"type": "string"
				},
				"type": {
					"type": "string",
					"enum": [
						"text",
						"json",
						"hex",
						"base64"
					]
				},
				"qos": {
					"type": "number",
					"enum": [
						0,
						1,
						2
					]
				},
				"retain": {
					"type": "boolean"
				},
				"properties": {
					"type": "object",
					"properties": {
						"payloadFormatIndicator": {
							"type": "boolean"
						},
						"messageExpiryInterval": {
							"type": "integer",
							"minimum": 0,
							"maximum": 9007199254740991
						},
						"topicAlias": {
							"type": "integer",
							"minimum": -9007199254740991,
							"maximum": 9007199254740991
						},
						"responseTopic": {
							"type": "string"
						},
						"correlationData": {
							"type": "string"
						},
						"subscriptionIdentifier": {
							"type": "integer",
							"minimum": -9007199254740991,
							"maximum": 9007199254740991
						},
						"contentType": {
							"type": "string"
						},
						"userProperties": {
							"type": "array",
							"items": {
								"type": "object",
								"properties": {
									"key": {
										"type": "string"
									},
									"value": {
										"type": "string"
									}
								},
								"required": [
									"key",
									"value"
								],
								"additionalProperties": false
							}
						}
					},
					"additionalProperties": false
				}
			},
			"required": [
				"payload",
				"topic",
				"type"
			],
			"additionalProperties": false
		},
		"children": {
			"maxItems": 0,
			"type": "array",
			"items": {
				"not": {}
			}
		},
		"extensions": {
			"type": "object",
			"properties": {},
			"additionalProperties": false
		}
	},
	"required": [
		"type",
		"payload"
	],
	"additionalProperties": false
}