{
  "name": "@postman/runtime",
  "version": "0.15.0",
  "license": "UNLICENSED",
  "description": "A complete Runtime that contains all ItemTypes supported by Postman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/postmanlabs/unified-runtime-monorepo.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "homepage": "https://github.com/postmanlabs/unified-runtime-monorepo/tree/master/packages/core/runtime#readme",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/index.node.d.ts",
        "default": "./dist/index.node.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./next": {
      "node": {
        "types": "./dist/next.node.d.ts",
        "default": "./dist/next.node.js"
      },
      "default": {
        "types": "./dist/next.d.ts",
        "default": "./dist/next.js"
      }
    },
    "./collection": {
      "types": "./dist/items/collection/index.d.ts",
      "default": "./dist/items/collection/index.js"
    },
    "./http": {
      "types": "./dist/items/http/index.d.ts",
      "default": "./dist/items/http/index.js"
    },
    "./graphql": {
      "types": "./dist/items/graphql/index.d.ts",
      "default": "./dist/items/graphql/index.js"
    },
    "./websocket": {
      "types": "./dist/items/websocket/index.d.ts",
      "default": "./dist/items/websocket/index.js"
    },
    "./mcp": {
      "types": "./dist/items/mcp/index.d.ts",
      "default": "./dist/items/mcp/index.js"
    },
    "./variables": {
      "types": "./dist/plugins/variables/index.d.ts",
      "default": "./dist/plugins/variables/index.js"
    },
    "./auth": {
      "types": "./dist/plugins/auth/index.d.ts",
      "default": "./dist/plugins/auth/index.js"
    },
    "./auth/*": {
      "types": "./dist/plugins/auth/*/index.d.ts",
      "default": "./dist/plugins/auth/*/index.js"
    },
    "./proxy": {
      "types": "./dist/plugins/proxy/index.d.ts",
      "default": "./dist/plugins/proxy/index.js"
    },
    "./certificates": {
      "types": "./dist/plugins/certificates/index.d.ts",
      "default": "./dist/plugins/certificates/index.js"
    },
    "./cookies": {
      "types": "./dist/plugins/cookies/index.d.ts",
      "default": "./dist/plugins/cookies/index.js"
    },
    "./lib/character-encoding": {
      "types": "./dist/lib/character-encoding.d.ts",
      "default": "./dist/lib/character-encoding.js"
    },
    "./server-methods": {
      "types": "./dist/server-methods.node.d.ts",
      "default": "./dist/server-methods.node.js"
    },
    "./platform": {
      "node": {
        "types": "./dist/platform.node.d.ts",
        "default": "./dist/platform.node.js"
      },
      "default": {
        "types": "./dist/platform.d.ts",
        "default": "./dist/platform.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@postman/node-oauth1": "1.4.0",
    "aws4fetch": "^1.0.20",
    "des.js": "^1.1.0",
    "file-type": "^16.5.4",
    "graphql": "^16.10.0",
    "graphql-ws": "^6.0.6",
    "jose": "^5.10.0",
    "json-bigint-string": "^1.0.0",
    "mutative": "^1.1.0",
    "postman-collection": "^5.2.0",
    "postman-request": "2.88.1-postman.46",
    "postman-sandbox": "^6.2.2",
    "postman-url-encoder": "^3.0.8",
    "postman-ws": "7.4.5-postman.1",
    "qs": "^6.14.0",
    "strip-json-comments": "^3.1.1",
    "undici": "^7.16.0",
    "zod": "^3.25.67",
    "@postman/runtime.auth": "^0.2.2",
    "@postman/runtime.collection": "^0.1.15",
    "@postman/runtime.core": "^0.5.1",
    "@postman/runtime.documentation": "^0.1.15",
    "@postman/runtime.dynamic-variables": "^0.2.0",
    "@postman/runtime.events": "^0.4.5",
    "@postman/runtime.folder": "^0.1.15",
    "@postman/runtime.grpc-client": "^0.2.9",
    "@postman/runtime.grpc-request": "^0.4.7",
    "@postman/runtime.grpc-utils": "^0.3.2",
    "@postman/runtime.models": "^0.10.1",
    "@postman/runtime.mqtt-client": "^0.3.2",
    "@postman/runtime.mqtt-request": "^0.4.2",
    "@postman/runtime.protocol-utils": "^0.0.1",
    "@postman/runtime.runtime-rpc": "^0.2.0",
    "@postman/runtime.schema": "^0.1.15",
    "@postman/runtime.socket.io-client": "^0.1.4",
    "@postman/runtime.socket.io-request": "^0.2.0",
    "@postman/runtime.variables": "^0.1.15",
    "@postman/runtime.websocket-client": "^0.1.1",
    "@postman/runtime.websocket-request": "^0.1.7"
  },
  "devDependencies": {
    "@types/node": "^22.18.0",
    "teleport-javascript": "^1.0.0",
    "@postman/test-servers": "^0.5.2"
  },
  "files": [
    "*.js",
    "*.json",
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf node_modules dist tsconfig.tsbuildinfo package-lock.json",
    "test": "vitest",
    "test:unit": "vitest --project @postman/runtime:unit",
    "test:e2e": "vitest --project @postman/runtime:e2e",
    "test-servers": "test-servers",
    "xray": "node --no-warnings --import ./scripts/xray.ts"
  }
}